docker.enabled = true
process.container = 'levim/vcf2maf:1.0'

params {
  somatic_vcf = false
  tumour_id = false
  normal_id = false
  vep_cache_version = 89
  vep_forks = 2
  buffer_size = 200
  species = 'homo_sapiens'
  outdir = 'results'
  genomes {
    'GRCh37' {
      fasta = 'NO_FILE'
      fai = 'NO_FILE1'
      filter_vcf = 'NO_FILE2'
      vep_path = 'NO_FILE3'
    }
    'GRCh38' {
      fasta = 's3://ngi-igenomes/igenomes/Homo_sapiens/GATK/GRCh38/Sequence/WholeGenomeFasta/Homo_sapiens_assembly38.fasta'
      fai = 's3://ngi-igenomes/igenomes/Homo_sapiens/GATK/GRCh38/Sequence/WholeGenomeFasta/Homo_sapiens_assembly38.fai'
      filter_vcf = false
      vep_path = false
    }
  }
}